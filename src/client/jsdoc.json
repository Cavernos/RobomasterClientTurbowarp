{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
        "template": "node_modules/docdash"
    },
    "plugins": ["node_modules/jsdoc-babel", "plugins/markdown"],
    "source": {
        "include": [
            ".",
            "./src",
            "./src/locales",
            "./src/types/locales",
            "./src/types/scratch"
        ],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "exclude": ["./eslint.config.js"]
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu": {
            "Gihub Repo": {
                "href": "",
                "target": "_blank",
                "class": "menu-item",
                "id": "repository"
            }
        }
    },
    "templates": {
        "default": {
            "useLongnameInNav": true
        }
    },
    "sourceType": "module"
}
